<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/linear_income_tax_b"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/itr_bussiness_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButtonIncomeTaxbussiness"
            android:layout_gravity="center"/>

        <TextView
            android:id="@+id/service"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="ITR-Bussiness"
            android:textColor="@color/black"
            android:layout_gravity="center"
            android:gravity="center"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_marginTop="8dp"/>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linear_income_tax_s"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="65dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/itr_salary_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButtonITRSalary"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton"/>

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="ITR-Salary"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_marginTop="8dp"/>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linear_gst_registration"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="55dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/gst_reg_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButtonGstRegistration"
            app:layout_constraintStart_toEndOf="@+id/imageButton2"
            android:layout_gravity="center"/>

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="GST Registration"
            android:textColor="@color/black"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_marginTop="8dp"/>

    </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="6dp">

        <LinearLayout
            android:id="@+id/linear_MEMS"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonMEMS"
                android:layout_width="67dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="@color/zxing_transparent"
                android:src="@drawable/msme_logo_new"
                android:scaleType="fitXY"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="8dp"
                android:text="MSME"
                android:textColor="@color/black"
                android:fontFamily="@font/nexa_light"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_proj_bal_sheet"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonbal_shet"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="@color/zxing_transparent"
                android:scaleType="fitXY"
                android:src="@drawable/balance_sheet_logo_new"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/landline_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="8dp"
                android:text="Balance Sheet"
                android:textColor="@color/black"
                android:fontFamily="@font/nexa_light"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_gst_return"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_marginTop="10dp">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/gst_return_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButtonGSTReturn"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton"/>

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="GST Return"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>
    </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="6dp">

        <LinearLayout
            android:id="@+id/linear_Gst_surrender"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonWater"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="@color/zxing_transparent"
                android:scaleType="fitXY"
                android:src="@drawable/gst_surrender_logo_new"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_water"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="8dp"
                android:text="GST Surrender"
                android:textColor="@color/black"
                android:fontFamily="@font/nexa_light"
                android:textStyle="bold"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_private_lmt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonPrivateLmt"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="@color/zxing_transparent"
                android:scaleType="fitXY"
                android:src="@drawable/privacy_limit_logo_new"
                app:layout_constraintStart_toEndOf="@+id/imageButton"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="8dp"
                android:text="Private limited"
                android:textColor="@color/black"
                android:fontFamily="@font/nexa_light"
                android:textStyle="bold"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
        android:id="@+id/linear_opc"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/opc_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButtonFastTag"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton2"
            />

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="OPC"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>

    </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="6dp">

        <LinearLayout
            android:id="@+id/linear_gst_amendment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="70dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="@color/zxing_transparent"
                android:src="@drawable/gst_amendment_logo_new"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="8dp"
                android:text="GST Amendment"
                android:textColor="@color/black"
                android:fontFamily="@font/nexa_light"
                android:textStyle="bold"
                android:textSize="14sp" />
        </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_gst_return4"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/gst_return_logo_new"
            android:scaleType="fitXY"
            android:background="@color/zxing_transparent"
            android:id="@+id/imageButtonI"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton"
            />

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="GST Return 4"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_e_way_bill"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="65dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/e_way_bill_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageButto"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton2"
            />

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="E Way Bill"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>

    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="6dp">


    <LinearLayout
        android:id="@+id/linear_add_director"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        >

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/add_director_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageBu"
            android:layout_gravity="center"/>

        <TextView
            android:id="@+id/txt_wa"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Add Director"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_accounting_in_tally"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/tally_acc"
            android:background="@drawable/background_oval10"
            android:id="@+id/imageTally"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton"/>

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Accounting in tally"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_dsc_token"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="60dp"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/dsc_logo_new"
            android:background="@color/zxing_transparent"
            android:scaleType="fitXY"
            android:id="@+id/imageB"
            android:layout_gravity="center"
            app:layout_constraintStart_toEndOf="@+id/imageButton2"
            />

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="DSC"
            android:fontFamily="@font/nexa_light"
            android:textStyle="bold"
            android:textSize="14sp"
            android:gravity="center"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="8dp"/>

    </LinearLayout>
    </LinearLayout>

</LinearLayout>