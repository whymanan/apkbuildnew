                        android:layout_marginTop="20dp"
                        android:textColor="@color/black"
                        android:layout_alignParentTop="true"
                        android:textSize="18sp"/>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_name_CR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/txt_dirct_name_CR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_nameCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/name_of_per"
                            android:textColor="@color/black"

                            android:textColorHint="@color/black"
                            android:inputType="textEmailAddress" />
                    </com.google.android.material.textfield.TextInputLayout>



                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_emailCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_name_CR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp"
                        >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_emailDCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/email_new"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="textEmailAddress" />
                    </com.google.android.material.textfield.TextInputLayout>



                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_mobile_noCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_emailCR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp"
                        >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_mobile_noDCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/mobile_no"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="phone" />
                    </com.google.android.material.textfield.TextInputLayout>



                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_f_h_nameCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_mobile_noCR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp"
                        >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_f_h_nameCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/f_h_name"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="textPersonName" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_dobCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_f_h_nameCR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp"
                        >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_dobCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/dob"
                            android:focusable="false"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:drawableRight="@drawable/ic_baseline_calendar_today_24"
                            android:inputType="number" />
                    </com.google.android.material.textfield.TextInputLayout>



                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_pan_no_CR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_dobCR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_pan_no_CR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/pan_no"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="number" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_adhar_no_CR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_pan_no_CR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_adhar_no_CR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/adhar_no"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="number" />
                    </com.google.android.material.textfield.TextInputLayout>



                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_addrCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_adhar_no_CR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_addrDCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="@string/addr"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="textPostalAddress" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/input_layout_din_noCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/input_layout_addrCR"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginTop="10dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/input_edt_din_noCR"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:hint="Designation"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:inputType="textPostalAddress" />
                    </com.google.android.material.textfield.TextInputLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/relative_photosCR"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/relative_direct_form_CR"
                    android:layout_marginTop="20dp">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_view_photoCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:cardBackgroundColor="@color/white"
                        app:cardElevation="10dp"
                        app:contentPaddingTop="20dp"
                        android:layout_alignParentTop="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/txt_photo_titleCR"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/photo"
                                android:textColor="@color/black"
                                android:layout_marginLeft="10dp"
                                android:textSize="20sp" />

                            <LinearLayout
                                android:id="@+id/linear_up_photoCR"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_below="@+id/txt_photo_titleCR"
                                android:orientation="horizontal">


                                <Button
                                    android:id="@+id/btn_photo_choseCR"
                                    android:text="Choose File"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="5dp"
                                    android:textColor="@color/white"
                                    android:background="@color/appPrimaryColor"
                                    android:layout_weight=".40" />

                                <TextView
                                    android:id="@+id/txt_photoCR"
                                    android:text="No file Choosen"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="5dp"
                                    android:textColor="@color/black"
                                    android:layout_weight=".60" />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_view_adhar_fCR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:cardBackgroundColor="@color/white"
                        app:cardElevation="10dp"
                        app:contentPaddingTop="20dp"
                        android:layout_below="@+id/card_view_photoCR"
                        android:layout_marginTop="5dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/txt_adhar_f_titleCR"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/adhr_f_s"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:textSize="20sp" />

                            <LinearLayout
                                android:id="@+id/linear_adhar_fCR"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_below="@+id/txt_adhar_f_titleCR"
                                android:orientation="horizontal">


                                <Button
                                    android:id="@+id/btn_adhar_f_g_choseCR"
                                    android:text="Choose File"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="5dp"
                                    android:background="@color/appPrimaryColor"
                                    android:textColor="@color/white"
                                    android:layout_weight=".40" />

                                <TextView
                                    android:id="@+id/txt_adhar_f_CR"
                                    android:text="No file Choosen"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="5dp"
                                    android:textColor="@color/black"
                                    android:layout_weight=".60" />
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_view_adhar_b_CR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:cardBackgroundColor="@color/white"
                        app:cardElevation="10dp"
                        app:contentPaddingTop="20dp"
                        android:layout_below="@+id/card_view_adhar_fCR"
                        android:layout_marginTop="5dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/txt_adhar_b_titleCR"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/adhar_b_s"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:textSize="20sp" />

                            <LinearLayout
                                android:id="@+id/linear_adhar_b_CR"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_below="@+id/txt_adhar_b_titleCR"
                                android:orientation="horizontal">


                                <Button
                                    android:id="@+id/btn_adhar_b_g_choseCR"
                                    android:text="Choose File"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:background="@color/appPrimaryColor"
                                    android:textColor="@color/white"
                                    android:layout_marginRight="5dp"
                                    android:layout_weight=".40" />

                                <TextView
                                    android:id="@+id/txt_adhar_b_CR"
                                    android:text="No file Choosen"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="5dp"
                                    android:textColor="@color/black"
                                    android:layout_weight=".60" />

                            </LinearLayout>
                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

